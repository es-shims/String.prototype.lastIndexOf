{
	"name": "string.prototype.lastindexof",
	"version": "0.0.0",
	"description": "An ES-spec-compliant `String.prototype.lastIndexOf`shim/polyfill/replacement that works as far down as ES3.",
	"main": "index.js",
	"exports": {
		".": [
			{
				"import": "./index.mjs",
				"require": "./index.js",
				"default": "./index.js"
			},
			"./index.js"
		],
		"./auto": "./auto.js",
		"./implementation": "./implementation.js",
		"./polyfill": "./polyfill.js",
		"./shim": "./shim.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"lint": "eslint --ext=js,mjs .",
		"postlint": "evalmd README.md",
		"pretest": "npm run lint",
		"tests-only": "nyc tape 'test/**/*.js'",
		"tests-esm": "nyc node test/index.mjs",
		"test": "npm run tests-only && npm run tests-esm",
		"posttest": "aud --production"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/es-shims/String.prototype.lastIndexOf.git"
	},
	"keywords": [
		"javascript",
		"ecmascript",
		"polyfill",
		"shim",
		"string",
		"lastindexof",
		"indexof",
		"index",
		"es-shim API"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/es-shims/String.prototype.lastIndexOf/issues"
	},
	"homepage": "https://github.com/es-shims/String.prototype.lastIndexOf#readme",
	"dependencies": {
		"es-abstract": "^1.18.6"
	},
	"devDependencies": {
		"@ljharb/eslint-config": "^18.0.0",
		"aud": "^1.1.5",
		"eslint": "^7.32.0",
		"evalmd": "^0.0.19",
		"functions-have-names": "^1.2.2",
		"has-strict-mode": "^1.0.1",
		"nyc": "^10.3.2",
		"tape": "^5.3.1"
	},
	"testling": { "files": "test/shimmed.js" }
}
